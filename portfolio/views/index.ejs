    <section id="contact" class="contact fade-in">
      <h2 class="section-title">Get in Touch</h2>
      <div class="contact-content fade-in"  data-aos-delay="200">
        <form id="contact-form" netlify novalidate>
          <!-- Honeypot field for spam bots -->
          <input type="text" name="_hp" id="_hp" autocomplete="off" tabindex="-1" aria-hidden="true" style="position:absolute;left:-10000px;top:auto;width:1px;height:1px;overflow:hidden;" />
          <div class="form-group">
            <input type="text" id="name" name="name" required placeholder="Your Name">
          </div> 
          <div class="form-group">
            <input type="email" id="email" name="email" required placeholder="Your Email">
          </div>
          <div class="form-group">
            <textarea id="message" name="message" required placeholder="Your Message"></textarea>
          </div>
          <button type="submit" class="submit-button fade-in btn card">
            <div class="submitbtn">
              <span>Send Message</span>
              <i class="fas fa-paper-plane plane"></i>
            </div>
          </button>
          <p id="contact-status" role="status" aria-live="polite" style="margin-top:0.75rem"></p>
        </form>
      </div>
    </section>
