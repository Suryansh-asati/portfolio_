    <section id="projects" class="projects fade-in">
        <h2 class="section-title">My Projects</h2>
        <div class="project-grid">
          <% (projects || []).slice(0, 6).forEach(function(p) { %>
            <div class="project-card card" tabindex="0" data-aos-delay="100">
              <div class="flip-inner">
                <div class="flip-front">
                  <div class="project-image">
                    <img src="<%= p.image %>" alt="<%= p.title %>" loading="lazy" decoding="async">
                  </div>
                  <div class="front-title"><h3><%= p.title %></h3></div>
                </div>
                <div class="flip-back">
                  <div class="project-content">
                    <h3><%= p.title %></h3>
                    <p><%= p.description || '' %></p>
                    <div class="project-tech">
                      <% (p.tags || []).forEach(function(t){ %>
                        <span><%= t %></span>
                      <% }) %>
                    </div>
                    <div class="project-links">
                      <% if (p.links && p.links.github) { %>
                        <a href="<%= p.links.github %>" class="project-link"><i class="fab fa-github"></i> GitHub</a>
                      <% } %>
                      <% if (p.links && p.links.live) { %>
                        <a href="<%= p.links.live %>" class="project-link"><i class="fas fa-external-link-alt"></i> Live Demo</a>
                      <% } %>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          <% }) %>
        </div>
        <div class="projects-cta">
          <a href="/projects" class="btn card more-projects-btn" aria-label="See more projects">
            <i class="fas fa-folder-open"></i>
            <span>Click for more projects</span>
          </a>
        </div>
      </section>
    
